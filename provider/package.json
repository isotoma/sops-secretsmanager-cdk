{
    "name": "sops-secretsmanager-cdk-provider",
    "license": "MIT",
    "author": {
        "name": "Isotoma Limited",
        "email": "hello@isotoma.com",
        "url": "https://isotoma.com"
    },
    "scripts": {
        "get-sops": "wget https://github.com/mozilla/sops/releases/download/v3.5.0/sops-v3.5.0.linux -O ./sops && chmod a+x ./sops",
        "tsc": "tsc",
        "clean": "rm -rf build/",
        "compile": "npm run clean && npm run -- tsc --outDir build/ && npm run get-sops && cp -R ./sops ./build/",
        "quick-compile": "npm run -- tsc --outDir build/ && ([[ -f ./sops ]] || npm run get-sops) && cp ./sops ./build/",
        "test": "jest"
    },
    "dependencies": {
        "@types/node": ">=10"
    },
    "devDependencies": {
        "@types/jest": "~24.0.22",
        "typescript": ">=3.0.0",
        "aws-sdk": ">=2",
        "jest": "~24.9.0",
        "ts-jest": "24.1.0"
    }
}
